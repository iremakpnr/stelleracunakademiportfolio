@model List<stelleracunakademiportfolio.Models.tbl_contact>


@foreach (var item in Model)
{
    <div class="contact-infos">
        <div class="item">
            <i class="ti-location-pin"></i>
            <div class="">
                <h5>Adres</h5>
                <p> @item.adress</p>
            </div>
        </div>
        <div class="item">
            <i class="ti-mobile"></i>
            <div>
                <h5>Telefon No</h5>
                <p>@item.phone</p>
            </div>
        </div>
        <div class="item">
            <i class="ti-email"></i>
            <div class="mb-0">
                <h5>Email</h5>
                <p>@item.Email</p>
            </div>
        </div>
    </div>

}

    </div>
</div>

@foreach (var item in Model)
{


    <div id="map">
        <iframe src="@item.MapUrl"></iframe>
    </div>

}
