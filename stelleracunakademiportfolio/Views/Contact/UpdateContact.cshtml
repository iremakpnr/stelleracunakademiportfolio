@model stelleracunakademiportfolio.Models.tbl_contact
@{
    ViewBag.Title = "UpdateContact";
    Layout = "~/Views/Shared/_adminLayout.cshtml";
}

<h2>İletişim Bilgisi Güncelleme Sayfası</h2>

<form method="post">
    @Html.HiddenFor(x => x.contactID)
    <label>Telefon</label>
    @Html.TextBoxFor(x => x.phone, new { @class = "form-control" })
    <br />
    <label>Email</label>
    @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
    <br />
    <label>Adres</label>
    @Html.TextBoxFor(x => x.adress, new { @class = "form-control" })
    <br />
    <label>Harita Url</label>
    @Html.TextBoxFor(x => x.MapUrl, new { @class = "form-control" })
    <br />

    <button class="btn btn-outline-primary">Güncelle</button>

</form>